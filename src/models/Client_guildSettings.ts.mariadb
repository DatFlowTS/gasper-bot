import { Entity, Column, PrimaryColumn } from 'typeorm';

@Entity('settings')
export class guildSetting {
	@PrimaryColumn({ type: 'varchar', length: 255 })
	guild!: string;

	@Column({ type: 'varchar', default: 'g!', length: 25 })
	prefix: string;

	@Column({ type: 'array', default: '', length: 255 })
	moderators: string[];

	@Column({ type: 'varchar', default: '', length: 255 })
	kick_logchannel: string;

	@Column({ type: 'varchar', default: '', length: 255 })
	ban_logchannel: string;
}
